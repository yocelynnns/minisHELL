{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "C/C++: gcc build and debug active file",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/minishell",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        },
        {
          "description": "Pass SIGINT to program",
          "text": "handle SIGINT nostop print pass",
          "ignoreFailures": false
        },
        {
          "description": "Don't stop on SIGINT in child processes",
          "text": "set follow-fork-mode child",
          "ignoreFailures": false
        },
        {
          "description": "Additional signal handling setup",
          "text": "set detach-on-fork off",
          "ignoreFailures": false
        }
      ],
      "preLaunchTask": "debug with cleanup",
      "miDebuggerPath": "/usr/bin/gdb"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/hthant/Core/minishell/src/parsing",
      "program": "/home/hthant/Core/minishell/src/parsing/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}